{
  "expo": {
    "name": "Namaz Vakti-İslami Uygulama",
    "slug": "namaz-vakti",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#051a2d"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.namazvakti.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Namaz vakitlerini ve kıble yönünü hesaplamak için konumunuza ihtiyacımız var.",
        "NSLocationAlwaysUsageDescription": "Namaz vakitlerini ve kıble yönünü hesaplamak için konumunuza ihtiyacımız var.",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#051a2d"
      },
      "package": "com.namazvakti.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_GOOGLE_MAPS_API_KEY"
        }
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-location",
      "expo-sensors",
      "expo-build-properties"
    ],
    "extra": {
      "eas": {
        "projectId": "6fab1ad0-35f3-4207-b01f-f3379b182fe1"
      }
    },
    "owner": "yakupyavuz"
  }
}
